{
  "name": "Gatekeeper",
  "version": "0.0.19",
  "author": "Nic Barker",
  "description": "An electron based GUI for analysing flow cytometry data.",
  "main": "main.js",
  "scripts": {
    "start": "webpack-dev-server --config webpack.dev.js",
    "dist": "webpack --config webpack.production.js",
    "pack": "electron-builder --dir",
    "build": "electron-builder"
  },
  "build": {
    "appId": "gatekeeper",
    "asar": false,
    "mac": {
      "category": "utilities"
    },
    "files": [
      "*",
      "webpack-build/*"
    ]
  },
  "license": "ISC",
  "dependencies": {
    "area-polygon": "^1.0.1",
    "d3": "^4.12.0",
    "distance-to-polygon": "^2.0.2",
    "electron-is-dev": "^0.3.0",
    "fcs": "^0.0.7",
    "hsl-to-rgb-for-reals": "^1.1.0",
    "hull.js": "^0.2.10",
    "lodash": "^4.17.4",
    "ls": "^0.2.1",
    "mkdirp": "^0.5.1",
    "path": "^0.12.7",
    "pngjs": "^3.3.1",
    "point-in-polygon": "^1.0.1",
    "polygon-overlap": "^1.0.5",
    "react": "^16.4.1",
    "react-dom": "^16.4.1",
    "react-onclickoutside": "^6.7.1",
    "react-redux": "^5.0.7",
    "redux": "^3.7.2",
    "request": "^2.85.0",
    "rimraf": "^2.6.2",
